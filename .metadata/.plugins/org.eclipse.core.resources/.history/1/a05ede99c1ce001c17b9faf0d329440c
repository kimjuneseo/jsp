create table tbl_hosp_202108(
	HOSPCODE CHAR(4) PRIMARY KEY,
	HOSPNAME VARCHAR2(10),
	HOSPTEL VARCHAR2(10),
	HOSPADDR VARCHAR2(10)	
)

