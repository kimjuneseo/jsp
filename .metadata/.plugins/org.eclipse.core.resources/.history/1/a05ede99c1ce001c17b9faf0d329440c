CREATE TABLE TBL_VACCRESV_202108(
	RESVNO CHAR(8) PRIMARY KEY,
	JUMIN CHAR(14),
	VCODE CHAR(4),
	HOSPCODE CHAR(4),
	RESVDATE CHAR(8),
	RESVTIME CHAR(4)
)
